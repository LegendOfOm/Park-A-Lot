{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run SFML App (gdb)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/sfml_app.exe",
      "cwd": "${workspaceFolder}",
      "args": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
      "preLaunchTask": "build-sfml-64",
      "setupCommands": [
        {
          "description": "Enable pretty printing",
          "text": "-enable-pretty-printing"
        }
      ]
    }
  ]
}
