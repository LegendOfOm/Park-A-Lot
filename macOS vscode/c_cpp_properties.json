{
  "version": 4,
  "configurations": [
    {
      "name": "mac-clang",
      "compilerPath": "/usr/bin/clang++",
      "intelliSenseMode": "macos-clang-arm64",
      "cStandard": "c17",
      "cppStandard": "c++17",
      "includePath": [
        "${workspaceFolder}/**",
        "/opt/homebrew/include"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}",
          "/opt/homebrew/include"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ]
}
